{% extends 'base.twig' %}
{% block content %}
<div class="uk-section uk-section-small" data-template="woo-archive.twig">
  <div class="uk-container uk-container-large">
    <div class="uk-grid-large" uk-grid>
      {% include 'sidebar-shop.twig' %}
      <div id="MainProductArchive" class="uk-article uk-width-5-6@m">
        <div>
          {% include 'shop-breadcrumbs.twig' %}
          {% include 'shop-archive-title.twig' %}
          {% include 'shop-archive-description.twig' %}
          {% include 'shop-toolbar.twig' %}
          {% include 'loop-products.twig' %}
          {% include 'shop-pagination.twig' %}
          {% do action('woocommerce_after_main_content') %}
        </div>
      </div>
    </div>
  </div>
</div>
{% endblock  %}